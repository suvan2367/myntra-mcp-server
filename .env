# Server Configuration
PORT=9093
NODE_ENV=production

# Myntra API Configuration
MYNTRA_API_BASE=https://api.myntra.com/seller
MYNTRA_SELLER_ID=your_seller_id_here
MYNTRA_API_KEY=your_api_key_here
MYNTRA_API_SECRET=your_api_secret_here

# Redis Configuration (Optional - for distributed deployments)
#REDIS_URL=redis://localhost:6379

# Security Configuration
ALLOWED_ORIGINS=https://claude.ai,https://chatgpt.com,http://localhost:3000

# Logging Configuration
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=1000

# Health Check Configuration
HEALTH_CHECK_INTERVAL=30000